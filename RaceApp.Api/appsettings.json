{
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "ConnectionMode": "Emulator",
    "Emulator": {
      "ServiceEndpoint": "https://localhost:8081"
    }
  },
  "CosmosDb": {
    "DatabaseName": "raceapp",
    "CollectionNames": [
      {
        "Name": "drivers",
        "PartitionKey": "/category"
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Secrets": {
    "Mode": "UseLocalSecretStore"
  }
}